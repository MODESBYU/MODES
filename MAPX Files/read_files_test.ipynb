      "     |  \n",
      "     |                            {\n",
      "     |                                \"http\" : \"http://10.343.10.22:111\",\n",
      "     |                                \"https\" : \"https://127.343.13.22:6443\"\n",
      "     |                            }\n",
      "     |  ----------------------    ---------------------------------------------------------------\n",
